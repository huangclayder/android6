From 691d1f110bd5a6d29369d244de5a45c97abcf832 Mon Sep 17 00:00:00 2001
From: lijian <jian.li@freescale.com>
Date: Thu, 17 Jul 2014 10:55:23 +0800
Subject: [PATCH 1/2] ENGR00323022 Support ASF/AVI/FLV http streaming for
 SLEVK

-   need to extend mimetype in gallery

Signed-off-by: lijian <jian.li@freescale.com>
---
 AndroidManifest.xml | 7 +++++++
 1 file changed, 7 insertions(+)

diff --git a/AndroidManifest.xml b/AndroidManifest.xml
index 5ebdd07..348bef5 100644
--- a/AndroidManifest.xml
+++ b/AndroidManifest.xml
@@ -121,6 +121,13 @@
                 <data android:mimeType="video/3gp" />
                 <data android:mimeType="video/3gpp" />
                 <data android:mimeType="video/3gpp2" />
+                <data android:mimeType="video/avi" />
+                <data android:mimeType="video/x-flv" />
+                <data android:mimeType="video/x-msvideo" />
+                <data android:mimeType="video/x-ms-asf" />
+                <data android:mimeType="video/x-ms-wmv" />
+                <data android:mimeType="video/mpeg" />
+                <data android:mimeType="video/quicktime" />
              </intent-filter>
         </activity>
         <activity android:name="com.android.camera.DeleteImage"
