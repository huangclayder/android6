+		if(isEnable){
+			mTvState.setText(R.string.ble_on);
+		}else{
+			mTvState.setText(R.string.ble_off);
+		}
+		mBtnState.setChecked(isEnable);
+	}
+
+	@Override
+	public void initData() {
+		boolean isBleEnable = PrefUtils.getBoolean(mActivity, PrefUtils.BLE_STATE, false);
