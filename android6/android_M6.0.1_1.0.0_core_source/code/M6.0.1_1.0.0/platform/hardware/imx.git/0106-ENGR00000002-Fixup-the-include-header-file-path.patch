+++ b/libcamera/Android.mk
@@ -36,7 +36,7 @@ LOCAL_SHARED_LIBRARIES:= \
 LOCAL_C_INCLUDES += \
 	frameworks/base/include/binder \
 	frameworks/base/include/ui \
-	frameworks/base/camera/libcameraservice
