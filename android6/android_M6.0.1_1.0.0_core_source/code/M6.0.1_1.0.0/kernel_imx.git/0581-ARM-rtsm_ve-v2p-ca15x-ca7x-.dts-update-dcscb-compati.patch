+++ b/arch/arm/boot/dts/rtsm_ve-v2p-ca15x4-ca7x4.dts
@@ -173,7 +173,7 @@
 	};
 
 	dcscb@60000000 {
-		compatible = "arm,dcscb";
+		compatible = "arm,rtsm,dcscb";
 		reg = <0 0x60000000 0 0x1000>;
 	};
 
