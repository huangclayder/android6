From a0a17e858535539e26fdb108d782395f026325ac Mon Sep 17 00:00:00 2001
From: Jon Medhurst <tixy@linaro.org>
Date: Wed, 10 Apr 2013 09:22:36 +0100
Subject: [PATCH] configs: android: Remove obsolete config options

These options no longer exist.

Signed-off-by: Jon Medhurst <tixy@linaro.org>
---
 linaro/configs/android.conf | 3 ---
 1 file changed, 3 deletions(-)

diff --git a/linaro/configs/android.conf b/linaro/configs/android.conf
index aca2283..8d7628c 100644
--- a/linaro/configs/android.conf
+++ b/linaro/configs/android.conf
@@ -3,13 +3,11 @@ CONFIG_IPV6=y
 CONFIG_PANIC_TIMEOUT=0
 CONFIG_HAS_WAKELOCK=y
 CONFIG_WAKELOCK=y
-CONFIG_USER_WAKELOCK=y
 CONFIG_BLK_DEV_LOOP=y
 CONFIG_DM_CRYPT=y
 CONFIG_POWER_SUPPLY=y
 CONFIG_ANDROID_PARANOID_NETWORK=y
 CONFIG_NET_ACTIVITY_STATS=y
-CONFIG_FB_EARLYSUSPEND=y
 CONFIG_INPUT_MISC=y
 CONFIG_INPUT_UINPUT=y
 CONFIG_INPUT_GPIO=y
@@ -20,7 +18,6 @@ CONFIG_ANDROID=y
 CONFIG_ANDROID_BINDER_IPC=y
 CONFIG_ASHMEM=y
 CONFIG_ANDROID_LOGGER=y
-CONFIG_ANDROID_RAM_CONSOLE=y
 CONFIG_ANDROID_TIMED_OUTPUT=y
 CONFIG_ANDROID_TIMED_GPIO=y
 CONFIG_ANDROID_LOW_MEMORY_KILLER=y
