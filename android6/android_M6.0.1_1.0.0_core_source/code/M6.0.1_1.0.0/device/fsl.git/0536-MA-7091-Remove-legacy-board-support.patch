-#
-# Product-specific compile-time definitions.
-#
-
-include device/fsl/imx6/soc/imx6dq.mk
-include device/fsl/arm2_6dq/build_id.mk
-include device/fsl-proprietary/gpu-viv/fsl-gpu.mk
