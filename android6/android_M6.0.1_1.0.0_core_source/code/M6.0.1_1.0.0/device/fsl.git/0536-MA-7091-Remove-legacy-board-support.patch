-ifeq ($(BUILD_TARGET_FS),ubifs)
-TARGET_RECOVERY_FSTAB = device/fsl/arm2_6dq/fstab_nand.freescale
-# build ubifs for nand devices
-PRODUCT_COPY_FILES +=	\
-	device/fsl/arm2_6dq/fstab_nand.freescale:root/fstab.freescale
-else
-TARGET_RECOVERY_FSTAB = device/fsl/arm2_6dq/fstab.freescale
-# build for ext4
-PRODUCT_COPY_FILES +=	\
-	device/fsl/arm2_6dq/fstab.freescale:root/fstab.freescale
-endif # BUILD_TARGET_FS
-
-TARGET_BOOTLOADER_BOARD_NAME := ARM2
-
-BOARD_SOC_CLASS := IMX6
-PRODUCT_MODEL := ARM2-MX6DQ
-
-# Wifi
-BOARD_WLAN_VENDOR 			 := ATHEROS
-# for atheros vendor
-ifeq ($(BOARD_WLAN_VENDOR),ATHEROS)
-BOARD_WLAN_DEVICE := ar6003
-BOARD_HAS_ATH_WLAN := true
-BOARD_WLAN_ATHEROS_SDK := system/wlan/atheros/compat-wireless
-BOARD_WPA_SUPPLICANT_DRIVER := NL80211
-BOARD_HOSTAPD_DRIVER := NL80211
-WPA_SUPPLICANT_VERSION := VER_0_9_ATHEROS
-HOSTAPD_VERSION := VER_0_9_ATHEROS
-WIFI_DRIVER_MODULE_PATH         := "/system/lib/modules/ath6kl_sdio.ko"
