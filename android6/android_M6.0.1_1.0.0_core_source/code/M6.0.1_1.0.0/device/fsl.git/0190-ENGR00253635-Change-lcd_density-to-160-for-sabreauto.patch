From d06286f3260e3750cd7e4ec750d9e2ee07947ad0 Mon Sep 17 00:00:00 2001
From: "guoyin.chen" <guoyin.chen@freescale.com>
Date: Fri, 19 Apr 2013 16:24:17 +0800
Subject: [PATCH 190/602] ENGR00253635 Change lcd_density to 160 for sabreauto

Change lcd_density to 160, in order to pass the Holo test of CTS

Signed-off-by: guoyin.chen <guoyin.chen@freescale.com>
---
 sabreauto_6q/init.rc | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/sabreauto_6q/init.rc b/sabreauto_6q/init.rc
index d25bd6e..41ef46f 100644
--- a/sabreauto_6q/init.rc
+++ b/sabreauto_6q/init.rc
@@ -55,8 +55,8 @@ on boot
     # Set rotation to 270 to cofigure as portrait mode
     setprop ro.sf.hwrotation 0
 
-    # Set the density to 140dpi, default 128dpi is not good
-    setprop ro.sf.lcd_density 140
+    # Set the density to 160 dpi, default 128dpi is not good
+    setprop ro.sf.lcd_density 160
 
     # prepare for ril
     setprop gsm.ril.delay 15
-- 
