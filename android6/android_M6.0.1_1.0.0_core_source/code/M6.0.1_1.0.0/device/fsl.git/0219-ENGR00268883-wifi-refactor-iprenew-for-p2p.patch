+++ b/evk_6sl/init.rc
@@ -111,11 +111,6 @@ service wpa_supplicant /system/bin/wpa_supplicant -iwlan0 -Dnl80211
     disabled
     oneshot
 
-service iprenew_wlan0 /system/bin/dhcpcd -n
-    class main
-    disabled
-    oneshot
-
 on fs
 # mount ext4 partitions
     mount_all /fstab.freescale
